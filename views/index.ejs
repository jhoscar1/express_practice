<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% parks.forEach(function(park){ %>
      <h2><%= park.name %></h2>
      <h4> $<%= park.cost %></h4>
      <img src="<%= park.image_url%>">
    <% }) %>
  </body>
</html>
